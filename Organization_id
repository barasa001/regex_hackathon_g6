<!DOCTYPE html>
<html>
<body>

<button onclick="myFunction()">Click to open</button>

<script>
function myFunction() {
  let id = prompt("Enter organization ID:", "");
   let match =  /^[^a-z][0-9a-z]*[^0-9]$/; 
  if (match.test(id)) {
          alert("Organization ID ' "+ id +" ' looks fine");
        return true;
    }
    else {
  
        alert("Organization ID is invalid");
        return false;
    }
}
</script>

</body>
</html>
